        </geometry>
        <material  name="darkred">
            <color rgba="0.6 0.0 0.0 1"/>
        </material>
      </visual>
    </link>
    <joint name="base_joint" type="fixed">
        <parent link="base_link_roll"/>
        <child link="base_link"/>
    </joint>

    <link name='sim_ft_sensor'>
